<odoo>
    <record id="sale_expo_journal_ri" model="account.journal">
        <field name='name'>Expo Sales Journal</field>
        <field name='company_id' ref="base.main_company"/>
        <field name='type'>sale</field>
        <field name='code'>S0002</field>
        <field name='refund_sequence' eval="False"/>
    </record>
    <record id="m_task_product_1" model="product.product">
        <field name="name">Product 1</field>
        <field name="list_price">10</field>
        <field name="type">consu</field>
        <field name="image_1920" type="base64" file="m_task/static/src/img/product1.png"/>
    </record>
    <record id="m_task_product_2" model="product.product">
        <field name="name">Product 2</field>
        <field name="list_price">10</field>
        <field name="type">service</field>
        <field name="image_1920" type="base64" file="m_task/static/src/img/product1.png"/>
    </record>
    <record id="m_task_product_3" model="product.product">
        <field name="name">Product 3</field>
        <field name="list_price">15</field>
        <field name="type">consu</field>
        <field name="image_1920" type="base64" file="m_task/static/src/img/product1.png"/>
    </record>

</odoo>
